var twoSum = function(nums, target) {
    for(let i = 0; i < nums.length; i++){ 
        for(let j = i+1; j < nums.length; j++){ // starts after i to avoid repeats
            if(nums[i]+nums[j] == target){ // tests for solution
                let output = [i,j];
                return output;
            }
        }
    }
};